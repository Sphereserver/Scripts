[TYPEDEF e_candlelit_holiday_sign]
ON=@Create
   TAG.GumpID=0689
   MOREZ=1
ON=@ContextMenuRequest
   SRC.AddContextEntry 101,1158829	//Change Sign Text
   SRC.AddContextEntry 102,1155742	//Toggle: On/Off
ON=@ContextMenuSelect
   IF (<ARGN> == 101)
      TAG0.GumpID ++
      IF (<TAG0.GumpID> > 068c)
         TAG.GumpID=0689
      ENDIF
      IF (<TAG0.GumpID> == 0689)
         ARGS="Happy Holidays"
      ELIF (<TAG0.GumpID> == 068a)
         ARGS="Merry Christmas"
      ELIF (<TAG0.GumpID> == 068b)
         ARGS="Season's Greetings"
      ELIF (<TAG0.GumpID> == 068c)
         ARGS="Happy Hanukkah"
      ENDIF
      SRC.SYSMESSAGE The sign text has been set to: <ARGS>
   ELIF (<ARGN> == 102)
